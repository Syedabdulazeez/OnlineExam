<div class='container'>
<h1>Edit User</h1>
<%= form_with(model: @user, url: admin_user_path(@user), method: 'patch') do |form| %>
  <%= render 'user_form_fields', form: form %>

  <div class="form-group">
    <%= form.submit 'Update Admin', class: 'btn btn-primary' %>
    <%= link_to'Cancel', admin_users_path, class: 'btn btn-secondary' %>
  </div>
<% end %>
</div>
