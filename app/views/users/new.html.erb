<div class="container mt-5">
  <div class="row justify-content-center ">
    <div class="col-md-6">
      <div class="card shadow">
        <div class="card-body">
          <h2 class="card-title text-center mb-4">Sign Up</h2>
          <%= render "form"%>
          <h3 class="text-center">Login with</h3>
          <%= button_to 'Google', '/auth/google_oauth2', class: "btn btn-google btn-block" %>
          <%= button_to 'FaceBook', '/auth/facebook', class: "btn btn-google btn-block" %>
        </div>
      </div>
    </div>
  </div>
</div>